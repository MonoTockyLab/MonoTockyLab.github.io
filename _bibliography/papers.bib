@article{RN82,
	abstract = {Human T-cell leukemia virus type 1 (HTLV-1), a retrovirus which mainly infects CD4(+) T cells and causes adult T-cell leukemia/lymphoma (ATL), is primarily transmitted via direct cell-to-cell transmission. This feature generates a wide variety of infected clones in hosts, which are maintained via clonal proliferation, resulting in the persistence and survival of the virus. The maintenance of the pool of infected cells is achieved by sculpting the immunophenotype of infected cells and modulating host immune responses to avoid immune surveillance. Here, we review the processes undertaken by HTLV-1 to modulate and subvert host immune responses which contributes to viral persistence and development of ATL.},
	author = {Tan, B. J. Y. and Sugata, K. and Ono, M. and Satou, Y.},
	doi = {10.3389/fimmu.2022.991928},
	issn = {1664-3224 (Electronic) 1664-3224 (Linking)},
	journal = {Front Immunol},
	keywords = {Adult Humans *Human T-lymphotropic virus 1 *Leukemia-Lymphoma, Adult T-Cell Carcinogenesis Immunophenotyping T-Lymphocytes Hla-ii adult T cell leukemia/lymphoma (ATL) cancer immune response clonal persistence human T-cell leukemia virus type 1 (HTLV-1) immune escape viral immune response},
	pages = {991928},
	title = {HTLV-1 persistence and leukemogenesis: A game of hide-and-seek with the host immune system},
	type = {Journal Article},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/36300109},
	volume = {13},
	year = {2022},
	html = {https://doi.org/10.3389/fimmu.2022.991928}}

@article{RN90,
	abstract = {The blockade of the immune checkpoints PD-1 and CTLA-4 enhances T cell response. However, it is largely unknown how antigen-reactive T cells regulate their checkpoint expression in vivo and whether and how the checkpoint blockade can change activation dynamics of tumour-reactive T cells. To address this, here we used Nr4a3-Timer-of-cell-kinetics-and-activity (Tocky), which allows analysis of temporal changes of activated T cells following TCR signalling in vivo. By analysing melanoma-bearing Nr4a3 Tocky mice, we elucidate hidden dynamics of tumour-reactive T cells in the steady-state. Checkpoint blockade depleted highly activated effector Treg, while promoting unique effector T cell populations, and thus differentially modulating activation of tumour-reactive T cell populations. Furthermore, multidimensional analysis and seamless analysis of Tocky and scRNA-seq revealed a full spectrum of T cell dynamics in response to tumour burden and treatment with checkpoint blockade. Lastly, we propose a rational design of combinatorial therapy to further enhance T cell activities.Competing Interest StatementThe authors have declared no competing interest.},
	author = {Jehanne, Hassan and Elizabeth, Appleton and Bahire, Kalfaoglu and Malin, Pedersen and Jos{\'e}, Almeida-Santos and Hisashi, Kanemaru and Nobuko, Irie and Shane, Foo and Omnia, Reda and Benjy, J. Y. Tan and Il-mi, Okazaki and Taku, Okazaki and Yorifumi, Satou and Kevin, Harrington and Alan, Melcher and Masahiro, Ono},
	doi = {10.1101/2022.07.19.500582},
	journal = {bioRxiv},
	pages = {2022.07.19.500582},
	title = {Single-cell level temporal profiling of tumour-reactive T cells under immune checkpoint blockade},
	type = {Journal Article},
	url = {http://biorxiv.org/content/early/2022/07/20/2022.07.19.500582.abstract},
	year = {2022},
	html = {http://biorxiv.org/content/early/2022/07/20/2022.07.19.500582.abstract},
	html = {https://doi.org/10.1101/2022.07.19.500582}}

