@article{RN93,
	abstract = {Summary Foxp3+ T regulatory (Treg) cells promote immunological tumor tolerance, but how their immune-suppressive function is regulated in the tumor microenvironment (TME) remains unknown. Here, we used intravital microscopy to characterize the cellular interactions that provide tumor-infiltrating Treg cells with critical activation signals. We found that the polyclonal Treg cell repertoire is pre-enriched to recognize antigens presented by tumor-associated conventional dendritic cells (cDCs). Unstable cDC contacts sufficed to sustain Treg cell function, whereas T helper cells were activated during stable interactions. Contact instability resulted from CTLA-4-dependent downregulation of co-stimulatory B7-family proteins on cDCs, mediated by Treg cells themselves. CTLA-4-blockade triggered CD28-dependent Treg cell hyper-proliferation in the TME, and concomitant Treg cell inactivation was required to achieve tumor rejection. Therefore, Treg cells self-regulate through a CTLA-4- and CD28-dependent feedback loop that adjusts their population size to the amount of local co-stimulation. Its disruption through CTLA-4-blockade may off-set therapeutic benefits in cancer patients.},
	author = {Marangoni, Francesco and Zhakyp, Ademi and Corsini, Michela and Geels, Shannon N. and Carrizosa, Esteban and Thelen, Martin and Mani, Vinidhra and Pr{\"u}{\ss}mann, Jasper N. and Warner, Ross D. and Ozga, Aleksandra J. and Di Pilato, Mauro and Othy, Shivashankar and Mempel, Thorsten R.},
	doi = {https://doi.org/10.1016/j.cell.2021.05.027},
	issn = {0092-8674},
	journal = {Cell},
	keywords = {multiphoton intravital microscopy MP-IVM T regulatory cell Treg cell nuclear factor of activated T cells NFAT tumor tolerance CD28 cytotoxic T lymphocyte-associated protein 4 CTLA-4},
	number = {15},
	pages = {3998-4015.e19},
	title = {Expansion of tumor-associated Treg cells upon disruption of a CTLA-4-dependent feedback loop},
	type = {Journal Article},
	url = {https://www.sciencedirect.com/science/article/pii/S0092867421006577},
	volume = {184},
	year = {2021},
	html = {https://www.sciencedirect.com/science/article/pii/S0092867421006577},
